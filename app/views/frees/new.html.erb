<!DOCTYPE html> <!--　この文章がHTML文章であることを伝える要素です。-->
<html lang="ja"><!--言語の指定-->
  <head><!--ページに表示されない情報(サイトの裏側の設定)-->
    <meta charset="utf-8">
    <title>広告研究会</title><!--ブラウザのタブのタイトルを指定-->
    <%= stylesheet_link_tag "frees", :media => "all" %>
  </head>


  <body><!--ページの見た目を作る部分-->
    <section id="area-2">
    </section>

<div class="orange">
  <div class="all-orange">
      <div class="orange-edit">
        <div class="new-title-box">
            <h1>
              フリーペーパー投稿ページ
            </h1>
        </div>
      </div>

<div class="">

</div>


  </div>
</div>


<div class="orange">
  <div class="all-orange">

<%= form_for @free do |t| %>

  <div class="field">
    
    <%= t.label :catch %>
    <br>
    <%= t.text_field :catch, :size => 140 %>
    <br>
    <%= t.label :number %>
    <br>
    <%= t.text_field :number, :size => 140 %>
    <br>
    <%= t.label :image %>
    <br>
    <%= t.file_field :image %>
    <br>
    <%= t.label :member %>
    <br>
    <%= t.text_field :member, :size => 140 %>
    <br>
    <%= t.label :overview %>
    <br>
    <%= t.text_field :overview, :size => 140 %>
    <br>
    <%= t.label :company %>
    <br>
    <%= t.text_field :company, :size => 140 %>
    <br>
    <%= t.label :url %>
    <br>
    <%= t.text_field :url, :size => 140 %>
    <br>
    <%= t.label :images2 %>
    <br>
    <%= t.file_field :images2 %>
    <br>
    <%= t.label :images3 %>
    <br>
    <%= t.file_field :images3 %>
    <br>
    <%= t.label :images4 %>
    <br>
    <%= t.file_field :images4 %>
    <br>
    <%= t.label :images5 %>
    <br>
    <%= t.file_field :images5 %>

    </div>

    <div class="edit-submit-delete-box">
  <div class="edit-submit-delete"><%= t.submit "フリペ投稿をする" %>
<% end %></div>
  
</div>
</div>
</div>

<script src="https://code.jquery.com/jquery-3.7.1.min.js" integrity="sha256-/JqT3SQfawRcv/BIHPThkBvs0OEvtFFmqPF/lYI/Cxo=" crossorigin="anonymous"></script>
  </body>
</html>